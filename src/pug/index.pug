extends _layout

block title
	title Index
block config
	- var main = true
block content
	script(src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.min.js")
	.main-screen-wrapper
		.main-video-wrapper(data-src="/img/index/main-screen-video.mp4")
			img(src="/img/index/video-poster.jpg", alt="").video-poster
		.main-screen
			.screen-links-wrapper
				.screen-link
					.screen-link-text.animated
						a(href="#").screen-link-title Инновации
						p Мы производим 33% всего <br> мяса птицы в стране
					img(src="/img/index/screen-link.jpg", alt="Инновации")
					.line.animeteThis.animated
				.screen-link
					.screen-link-text.animated
						a(href="#").screen-link-title Экспертность
						p Мы производим 33% всего <br> мяса птицы в стране
					img(src="/img/index/screen-link.jpg", alt="Экспертность")
					.line.animeteThis.animated.lineTwo
				.screen-link
					.screen-link-text.animated
						a(href="#").screen-link-title Лидерство
						p Мы производим 33% всего мяса птицы в стране
					img(src="/img/index/screen-link.jpg", alt="Лидерство")
			.main-screen-title
				.title-wrapper.animated.fadeInDownMenu
					p.title-sup Группа компаний
					p.title-main Серволюкс
	#main-content
		.box
			.block
				.flex-block
					.one-half.animateThis(data-anim="fadeInLeft")
						h2 Наши бренды
					.one-half.animateThis(data-anim="fadeInRight")
						.text-wrap
							p.annotation Продукция группы компаний «Серволюкс»  пользуется высоким спросом не только у белорусских потребителей — до 50% реализуется на экспортных рынках.
							a(href="#").green-arrow Все бренды
								.green-circle
				include includes/_brands.pug
		.box.gray
			.block
				.flex-block
					.one-half.animateThis(data-anim="fadeInLeft")
						.text-wrap
							h2 «Серволюкс» — группа компаний, состоящая из 15 предприятий-лидеров агропромышленного комплекса Беларуси
